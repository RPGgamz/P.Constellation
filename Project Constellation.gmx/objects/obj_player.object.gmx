var hydroloss = .04;

if (distance_to_object(obj_oasis) &lt; 16) {
    near_oasis = true;
} else {
    near_oasis = false;
}

if (obj_cycle.sandstorm) {
    hydroloss += .06;
}
if !(near_oasis) {
    hydro -= hydroloss;
} else {
    if (hydro &lt; hydromax) {
        hydro += 1;
        if (hydro &gt; hydromax) {
            hydro = hydromax;
        }
    }
}

